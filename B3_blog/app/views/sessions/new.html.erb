<div class="container"style="padding: 100px 300px 10px;">
<div class="jumbotron">
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(:session,url: login_path) do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username %><br>&nbsp; &emsp;
    <%= f.label :email %>
    <%= f.text_field :email %><br>
    <%= f.label :password %>
    <%= f.password_field :password %><br> &emsp; &emsp; &emsp; &emsp;&emsp; &emsp;&emsp; &emsp;
      <%= f.submit "login_in" %>
     <% end %>
  </div>
</div>
</div>
</div>
